	file = {:home/gleb/Документы/Mendeley Desktop/Cooling and Autonomous Feedback in a Bose-Hubbard Chain with Attractive Interactions - Hacohen-Gourgy et al. - 2015.pdf:pdf},
	issn = {10797114},
